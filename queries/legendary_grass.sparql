#query legendary grass
SELECT DISTINCT ?pokemonName
WHERE {
  {?pokemon rdf:type pkmn:Pokemon ;
           pkmn:hasName ?pokemonName;
           pkmn:hasType1 "grass"
  FILTER(LANG(?pokemonName) = "en")}
  UNION
{
?pokemon rdf:type pkmn:Pokemon ;
           pkmn:hasName ?pokemonName;
           pkmn:hasType2 "grass"
  FILTER(LANG(?pokemonName) = "en")
}}
